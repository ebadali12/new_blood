    <!--android:id="@+id/map"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent"-->
    <!--android:name="com.google.android.gms.maps.SupportMapFragment"-->
    <!--map:mapType="normal"/>-->




    <FrameLayout
        android:id="@+id/frame_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbars">


    </FrameLayout>






</LinearLayout>






